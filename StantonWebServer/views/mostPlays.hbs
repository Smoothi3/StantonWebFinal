<h1>MOST Plays PAGE</h1>
<body>
    <div id="container">
        <p>High Scores Ordered Below</p>
        <script>
            fetch('/getMostPlays').then(function(scores){
                scores.json().then(function(thescores){
                    console.log(thescores)

                    for(let score of thescores.myPlays){
                        var scoreElement = document.createElement('p')
                        scoreElement.innerHTML = score.myName + " : Times Played : " + score.myPlays
                        document.getElementById('container').appendChild(scoreElement)
                    }
                })
            })
        </script>
    </div>
    <a href="/">Main Page</a>
</body>